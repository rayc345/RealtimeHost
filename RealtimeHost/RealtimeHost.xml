<?xml version="1.0" encoding="utf-8"?>
<Application xmlns="http://schemas.microsoft.com/windows/2009/Ribbon">
	<Application.Commands>
		<Command Name="cmdQAT" Symbol="ID_QAT"/>
		<Command Name="cmdCustomizeQAT" Symbol="ID_CUSTOM_QAT"/>
		<Command Name="cmdHelp" Symbol="IDR_CMD_HELP">
			<Command.LabelTitle>
